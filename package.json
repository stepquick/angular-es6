{
  "name": "angular150",
  "version": "1.0.0",
  "description": "Angular 1.5.0 in ES6",
  "main": "src/main.js",
  "scripts": {
    "watch": "npm run watch:dev",
    "watch:dev": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:prod": "webpack --watch --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached",
    "build": "npm run build:dev",
    "build:dev": "webpack --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "webpack --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached --bail",
    "server": "npm run server:dev:hmr",
    "server:dev": "webpack-dev-server --inline --progress --profile --colors --watch --display-error-details --display-cached",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:prod": "npm run build:prod && http-server dist --cors",
    "start": "npm run server"
  },
  "keywords": [
    "angular",
    "es6"
  ],
  "private": true,
  "author": "Stephen Quick",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.0",
    "angular-ui-bootstrap": "^1.2.4",
    "angular-ui-router": "^0.2.18",
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.6.5",
    "babel-runtime": "^6.6.1",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.10.0",
    "http-server": "^0.9.0",
    "open-browser-webpack-plugin": "0.0.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
